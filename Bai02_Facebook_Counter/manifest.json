{
  "name": "Facebook, Instagram Counter",
  "description": "Count visits and time usage on Facebook, Instagram",
  "version": "1.0.0",
  "browser_action": 
  {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "tabs", "storage", "cookies", "https://www.facebook.com/*" ],
  "background": 
  {
    "scripts": [ "background.js" ]
  },
  "manifest_version": 2
 }